  #size-cells = <1>;
  compatible = "jedec,spi-nor";
  reg = <0>;
  spi-max-frequency = <40000000>;

  fsbl@0 {

   label = "U-Boot-min";
   reg = <0 0x40000>;
  };
  ssbl@1 {

   label = "U-Boot";
   reg = <0x40000 0x80000>;
  };
  bootenv@2 {

   label = "U-Boot Env";
   reg = <0xc0000 0x40000>;
  };
  kernel@3 {

   label = "Kernel";
   reg = <0x100000 0x400000>;
  };
  ipmi@4 {
   label = "IPMI FRU";
   reg = <0x500000 0x40000>;
  };
  fs@5 {
   label = "File System";
   reg = <0x540000 0x1ac0000>;
  };
 };
};

